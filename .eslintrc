{
  "extends": "react-app",
  "plugins": ["react"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-console": [
      "error"
    ],
    "jsx-quotes": [
      "error",
      "prefer-double"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "prefer-destructuring": [
      "error", 
      { 
        "object": true, 
        "array": false
      }
    ],
    "react/destructuring-assignment": ["error","always"]
  }
}